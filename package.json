{
	"name": "short-link",
	"version": "1.0.0",
	"description": "A simple URL shortening service built with Express and TypeScript.",
	"author": "duyaivy",
	"repository": "duyaivy/SHORTEN_URL",
	"license": "MIT",
	"main": "index.ts",
	"private": true,
	"scripts": {
		"build": "tsc && tsup",
		"dev": "node --import=tsx --watch src/index.ts",
		"prod": "node dist/index.js",
		"check": "biome check --write",
		"test": "vitest run",
		"test:cov": "vitest run --coverage"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.887.0",
		"@aws-sdk/lib-storage": "^3.887.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/lodash": "^4.17.20",
		"@types/ms": "^2.1.0",
		"@types/nodemailer": "^7.0.1",
		"@types/qrcode": "^1.5.5",
		"axios": "^1.11.0",
		"cors": "2.8.5",
		"dotenv": "17.2.1",
		"express": "5.1.0",
		"express-rate-limit": "8.0.1",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"jsonwebtoken": "^9.0.2",
		"lib-storage": "github:aws-sdk/lib-storage",
		"lodash": "^4.17.21",
		"mongodb": "^6.19.0",
		"ms": "^2.1.3",
		"nodemailer": "^7.0.6",
		"pino": "9.9.0",
		"pino-http": "10.5.0",
		"qrcode": "^1.5.4",
		"uuid": "8.3.2",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.3",
		"@types/supertest": "6.0.3",
		"@types/uuid": "^10.0.0",
		"@vitest/coverage-v8": "3.2.4",
		"pino-pretty": "13.1.1",
		"supertest": "7.1.4",
		"tsup": "8.5.0",
		"tsx": "4.20.4",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"outDir": "dist",
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2020",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true
	},
	"packageManager": "pnpm@10.14.0"
}
